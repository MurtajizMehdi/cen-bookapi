@model List<Genre> 

<h1>Genre List</h1>
<table class="table table-bordered table-striped">
    <thread>
        <tr>
            <th>
                Genre Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thread>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>

        }


    </tbody>



</table>